{
  "name": "news-frontend-reactjs",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "env-cmd .env.itcrowdhu react-scripts start",
    "start:itcrowdhu": "env-cmd .env.itcrowdhu react-scripts start",
    "start:itnewscloud": "env-cmd .env.itnewscloud react-scripts start",
    "build": "react-scripts build",
    "build:itcrowdhu": "env-cmd .env.itcrowdhu react-scripts build  && mv build build_itcrowdhu",
    "build:itnewscloud": "env-cmd .env.itnewscloud react-scripts build  && mv build build_itnewscloud",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "https://itcrowd.hu",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "env-cmd": "^8.0.2"
  }
}
